{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "FeatureFlags": {
    "ProviderType": "EfCore", //Sql or EfCore
    "CacheDurationMinutes": 5
  },

  "Migrations": {
    "RunForAllTenants": false
  },


  "ConnectionStrings": {
    "Default": "Server=.\\SQLEXPRESS;Database=FeatureFlags_Default;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=false;TrustServerCertificate=True;",
    "Tenant1Db": "Server=.\\SQLEXPRESS;Database=FeatureFlags_Tenant1;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=false;TrustServerCertificate=True;",
    "Tenant2Db": "Server=.\\SQLEXPRESS;Database=FeatureFlags_Tenant2;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=false;TrustServerCertificate=True;"
  }
}

